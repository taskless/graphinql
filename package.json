{
  "name": "@taskless/graphinql",
  "description": "A minimalist GraphQL client based on phin",
  "version": "3.6.2",
  "author": {
    "name": "Taskless.io",
    "email": "oss@taskless.io",
    "url": "https://taskless.io"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "p-retry": "^5.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/node": "18.11.18",
    "ava": "^4.3.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "syncpack": "^10.6.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "^5.1.3",
    "xo": "^0.54.2"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "homepage": "https://github.com/taskless/taskless/",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": "taskless/taskless.git",
  "scripts": {
    "ava": "ava",
    "build": "run-s tsup",
    "clean": "shx rm -rf dist && shx mkdir dist",
    "dev": "tsup --watch",
    "prepare": "husky install",
    "test": "ava",
    "tsup": "tsup",
    "typecheck": "tsc --noEmit",
    "xo": "xo"
  },
  "tsup": {
    "entry": {
      "index": "src/index.ts"
    },
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": false
  },
  "type": "module",
  "types": "dist/index.d.ts"
}
